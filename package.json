{
  "name": "ics-calendar-server",
  "version": "1.0.0",
  "description": "Node.js back-end server application for creation of .ical/.ics calendar event files",
  "main": "index.js",
  "scripts": {
    "start": "node ./index.js",
    "mocha": "mocha ./tests/{*,/*}.spec.js --colors",
    "test": "nyc --reporter=html --port=2222 --reporter=text mocha ./tests/{*,/*}.spec.js --colors",
    "lint:install": "npm i babel-eslint eslint eslint-plugin-node eslint-plugin-standard",
    "lint": "./node_modules/.bin/eslint --ext .js libs",
    "lint:fix": "./node_modules/.bin/eslint --fix --ext .js libs"
  },
  "author": "eaglex",
  "license": "CC-BY-NC-ND",
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "mocha": "^3.5.3",
    "nyc": "^15.1.0"
  },

  "engine": {
    "node": "*",
    "npm": "*"
  },

  "dependencies": {
    "bash-color": "0.0.4",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "ejs": "^3.1.3",
    "eslint": "^7.3.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-standard": "^4.0.1",
    "express": "^4.17.1",
    "ics": "^2.22.1",
    "lodash": "^4.17.15",
    "moment": "^2.27.0",
    "morgan": "^1.10.0",
    "simple-q": "git+https://bitbucket.org/eag1ex/simple-q.git",
    "uuid": "^8.2.0",
    "x-units": "git+https://bitbucket.org/eag1ex/x-utils.git"
  }
